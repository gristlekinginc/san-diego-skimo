# Basic Jekyll gitignores
_site/
.sass-cache/
.jekyll-cache/
.jekyll-metadata

# Access Token refresh
scripts/strava_tokens.json

# Ignore Python cache files
__pycache__/
*.py[cod]
*.pyc

# Ignore environment configuration
.env

# Ignore intermediate or generated files
/templates/action-journal.html
/static/*.min.css
/static/*.min.js

# Ignore macOS system files
.DS_Store
._*
Thumbs.db

# Ignore logs and temporary files
*.log
*.tmp
*.bak

# Virtual environment
sdskimo-venv/

# Additional Python-related exclusions
*.ipynb_checkpoints/

# Additional Ruby/Bundler ignores
/vendor

# GitHub Pages specific exclusions
Gemfile.lock
